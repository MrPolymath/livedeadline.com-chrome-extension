{
  "manifest_version": 3,
  "name": "New Tab Countdown to Deadline",
  "description": "Set your new tab page to show a countdown to your next deadline",
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
